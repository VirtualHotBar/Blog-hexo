<footer class="footer" id="footer" style="background: var(--code-background);text-align: center;">
    <div class="info-container">
        <br/>
        
        <% if (theme.footer.hasOwnProperty('icp') && theme.footer.icp) { %>
            <div class="icp-info info-item"><a target="_blank" rel="nofollow" href="https://beian.miit.gov.cn">
                    <%= theme.footer.icp %>
                </a></div>
        <% } %>

        <% if (theme.footer.hasOwnProperty('moeicp') && theme.footer.moeicp) { %>
            <div class="icp-info info-item"><a target="_blank" rel="nofollow" href="https://icp.gov.moe/">
                <%= theme.footer.moeicp %>
            </a></div>
        <% } %>








        <% if (theme.website_count.busuanzi_count.enable===true) { %>
            <script async <%=theme.pjax.enable===true ? 'data-pjax' : ''
                %> src = "//busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js" ></script>
            <div class="website-count info-item">
                <% if (theme.website_count.busuanzi_count.site_uv) { %>
                    <span id="busuanzi_container_site_uv">
                        <%- __('site_uv') %>&nbsp;<span id="busuanzi_value_site_uv"></span>&ensp;
                    </span>
                    <% } %>
                        <% if (theme.website_count.busuanzi_count.site_pv) { %>
                            <span id="busuanzi_container_site_pv">
                                <%- __('site_pv') %>&nbsp;<span id="busuanzi_value_site_pv"></span>
                            </span>
                            <% } %>
            </div>
        <% } %>


<!--         <% if (theme.footer.hasOwnProperty('runtime') && theme.footer.runtime) { %>
            <span id="RunTime"></span>
        <% } %> -->


        <% if (theme.footer.hasOwnProperty('upyun') && theme.footer.upyun) { %>
                    <div >
                        <a target="_blank" rel="nofollow"href="<%= theme.footer.upyun %>">本网站由
                            <img src="/images/upyun.png" alt="upyun" srcset="" height="24" style="position:relative; top:6px">
                            提供CDN加速/云存储服务
                        </a>
                    </div>
        <% } %>
    </div>
    <div style="border-top: 1px solid rgba(0, 0, 0, 0.1);padding: 10px 10px 10px;">
        © <span><%= theme.footer.since %></span>-<%= date(new Date(), 'YYYY' ) %> <a href="https://www.loser.plus/" target="_blank" class="class-link"
            rel="noopener">VirtualHotBar</a>
    </div>
</footer>